void print_remainig_days(int month, int day, int year)
{
  if ((year % 400 == 0) || (year % 4 == 0 && year % 100 != 0))
    {
      if (month >= 3 && day >= 60)
	{
	  day++;
	}

      printf("Day of the year: %d\n", day);
      printf("Remaining days: %d\n", 366 - day);
    }
  else
    {
      if (month == 2 && day == 60)
	}
  printf("Invalid date: 502d/%02d/%04d\n", month, day -31, year);
}
